#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/liuxiang/Documents/前端积累/vitepress-demo-mondaylab/node_modules/.pnpm/registry.npmmirror.com+vite@4.4.0-beta.3/node_modules/vite/bin/node_modules:/Users/liuxiang/Documents/前端积累/vitepress-demo-mondaylab/node_modules/.pnpm/registry.npmmirror.com+vite@4.4.0-beta.3/node_modules/vite/node_modules:/Users/liuxiang/Documents/前端积累/vitepress-demo-mondaylab/node_modules/.pnpm/registry.npmmirror.com+vite@4.4.0-beta.3/node_modules:/Users/liuxiang/Documents/前端积累/vitepress-demo-mondaylab/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/liuxiang/Documents/前端积累/vitepress-demo-mondaylab/node_modules/.pnpm/registry.npmmirror.com+vite@4.4.0-beta.3/node_modules/vite/bin/node_modules:/Users/liuxiang/Documents/前端积累/vitepress-demo-mondaylab/node_modules/.pnpm/registry.npmmirror.com+vite@4.4.0-beta.3/node_modules/vite/node_modules:/Users/liuxiang/Documents/前端积累/vitepress-demo-mondaylab/node_modules/.pnpm/registry.npmmirror.com+vite@4.4.0-beta.3/node_modules:/Users/liuxiang/Documents/前端积累/vitepress-demo-mondaylab/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vite.js" "$@"
else
  exec node  "$basedir/../../bin/vite.js" "$@"
fi
